print(" Наибольшие числа")
n = int(input("Сколько чисел? (не менее 2): "))
max1 = int(input("Число 1: "))
max2 = -1

for i in range(2, n + 1):
    num = int(input(f"Число {i}: "))
    if num > max1:
        max2 = max1
        max1 = num
    elif num > max2:
        max2 = num

print(max1)
print(max2)
